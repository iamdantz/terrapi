<% if (provider === 'AWS') { %>
output "bucket_arn" {
  value = aws_s3_bucket.example.arn
}
<% } else if (provider === 'Azure') { %>
output "resource_group_id" {
  value = azurerm_resource_group.example.id
}
<% } else if (provider === 'GCP') { %>
output "bucket_url" {
  value = google_storage_bucket.example.url
}
<% } %>
